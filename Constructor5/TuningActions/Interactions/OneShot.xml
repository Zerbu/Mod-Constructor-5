<?xml version="1.0" encoding="utf-8"?>
<TuningActions>
  <V n="basic_content" t="one_shot">
    <U n="one_shot">
	    <IfBool p="HasAnimation">
		    <V n="animation_ref" t="enabled">
          <U n="enabled">
			      <T n="factory">$Animation</T>
		      </U>
		    </V>
	    </IfBool>
    </U>
  </V>
  <V n="outcome" t="single">
    <U n="single">
      <U n="actions">
        <L n="loot_list">
          <ForEachReference p="LootActionSets">
            <T>$v</T>
          </ForEachReference>
          <T>$LearnTraitLootKey</T>
        </L>
      </U>
    </U>
  </V>
</TuningActions>